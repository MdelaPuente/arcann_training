# Distances CMe-X
DISTANCE ATOMS=CMe,Cl LABEL=CV_d_CMeCl
DISTANCE ATOMS=CMe,Br LABEL=CV_d_CMeBr
COMBINE ARG=CV_d_CMeCl,CV_d_CMeBr PERIODIC=NO COEFFICIENTS=1,-1 LABEL=CV_Dd
DISTANCES ATOMS1=CMe,Cl ATOMS2=CMe,Br LOWEST HIGHEST LABEL=CV_ds_CMeClCMeBr

# Coordinations CMe-X
COORDINATION GROUPA=CMe GROUPB=Cl NN=8 MM=16 D_0=0 R_0=2.5 LABEL=CV_CN_CMeCl
COORDINATION GROUPA=CMe GROUPB=Br NN=8 MM=16 D_0=0 R_0=2.5 LABEL=CV_CN_CMeBr
COMBINE ARG=CV_CN_CMeCl,CV_CN_CMeBr PERIODIC=NO COEFFICIENTS=1,-1 LABEL=CV_DCN

# Distances C-H
DISTANCES GROUPA=CMe GROUPB=HMe LOWEST HIGHEST LABEL=CV_ds_CMeHMe
DISTANCES LOWMEM GROUPA=ANCN GROUPB=ANHMe LOWEST HIGHEST LABEL=CV_ds_ANCNANHMe

# Angle
ANGLE ATOMS=Cl,CMe,Br LABEL=CV_a_ClCMeBr
TORSION ATOMS=H1,H2,H3,CMe LABEL=CV_t_H1H2H3CMe

